{
  "name": "types",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "generate": "./node_modules/.bin/typescript-json-schema tsconfig.json \"*\" --defaultNumberType integer --required --out ../specification/json/a2a.json",
    "generate-types": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/types --ts_proto_opt=outputEncodeMethods=false,outputPartialMethods=false,outputJsonMethods=false,outputServices=false,emitImportedFiles=false,oneof=unions-value,stringEnums=true,env=node -I ./node_modules/google-proto-files -I ../specification/grpc ../specification/grpc/*.proto",
    "prettier": "prettier \"src/types/**/*.ts\" --write"
  },
  "author": "",
  "description": "A2A Types",
  "dependencies": {
    "@grpc/grpc-js": "^1.1.8",
    "@grpc/proto-loader": "^0.7.13",
    "google-proto-files": "^3.0.0",
    "typescript-json-schema": "^0.65.1"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "ts-proto": "2.7.5",
    "typescript": "^5.9.3"
  }
}
